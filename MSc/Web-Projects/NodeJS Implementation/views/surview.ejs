<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>My Survey</title>
  <script type="text/javascript" src="./javascripts/select_script.js"></script>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.3/css/bulma.min.css">
</head>

<body>
  <section class="section">
    <table>
      <tr>
        <th>Loki Id</th>
        <th>Cartoon Name</th>
        <th>Character Name</th>
        <th>Respondent Age</th>
      </tr>
      <% surview.forEach(function (surrow) { %>
      <tr>
        <td><%= surrow.$loki %></td>
        <td><%= surrow.cartoon %> </td>
        <td><%= surrow.character %></td>
        <td><%= surrow.age %> </td>
      </tr>
      <% }) %>
    </table>
  </section>

</body>

</html>